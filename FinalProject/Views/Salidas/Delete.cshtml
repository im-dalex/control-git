@model ProyectoFinal.Models.Salidas

@{
    ViewBag.Title = "Delete";
}

<br>
<div class="container">
    <div class="row">
        <div class="col-md-4"></div>
        <div class="col-md-4">
            <div class="card">
                <div class="card-header bg-secondary text-white text-center">
                    <h5>Seguro de querer eliminar esta salida?</h5>
                </div>
                <div class="card-body">
                    <div>

                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.Empleados.codigoEmpleado)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Empleados.codigoEmpleado)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.tipoSalida)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.tipoSalida)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Motivo)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Motivo)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.fechaSalida)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.fechaSalida)
                            </dd>

                        </dl>

                        @using (Html.BeginForm())
                        {
                            @Html.AntiForgeryToken()

                            <div class="form-actions no-color">
                                <input type="submit" value="Borrar" class="btn btn-primary" /> |
                                @Html.ActionLink("Atras", "Index")
                            </div>
                        }
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
